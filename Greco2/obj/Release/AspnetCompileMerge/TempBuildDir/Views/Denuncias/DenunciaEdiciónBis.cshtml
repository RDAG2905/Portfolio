                        data: {
                            dni: $('#dni').val(),
                            nombre: $('#nombre').val(),
                            apellido: $('#apellido').val()
                        },
                        success: function (response) {
                            $('#loading').hide();
                            $('#denuncianteResult').html(response);
                            $('#denuncianteId').val($('#idDenuncianteResult').val());


