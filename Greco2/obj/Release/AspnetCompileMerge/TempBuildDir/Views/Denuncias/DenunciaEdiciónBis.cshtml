    function verificarContenidoNroLinea(elem) {
        var input = $(elem);
        var reg = /^\d+(\d+)?$/;

        if (input.val().trim().length > 0) {
            if (!reg.test(input.val())) {
                input.addClass('error');
                toastr.warning('El número de Linea <br/> solo puede contener Números');
            }
            else
            if (input.val().length != 10) {
                input.addClass('error');
                toastr.warning(' El número de Linea <br/> debe contener 10 dígitos');
            }
        }
    }

            function verificarContenidoDni(elem) {
                var input = $(elem);
                var reg = /^\d+(\d+)?$/;

                if (input.val().trim().length > 0) {
                    if (!reg.test(input.val())) {
                        input.addClass('error');
                        toastr.positionClass = 'toast-bottom-right';
                        toastr.options = {
                            maxOpened: 1,
                            preventDuplicates: 1,
                            autoDismiss: true
                        };
                        toastr.warning('El número de Documento <br/> solo puede contener Números');
                    }

                } else {
                    input.removeClass('error');
                }
            }


