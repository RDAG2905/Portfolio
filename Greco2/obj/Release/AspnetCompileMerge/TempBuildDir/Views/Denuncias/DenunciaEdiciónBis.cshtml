            function sonValidosLosDatosCoprec() {
                var flag = true;
                var regExp = /^\d+(?:,\d+)?$/;
                if ($('#inputHonorarios').val().trim().length > 0) {
                    if (!regExp.test($('#inputHonorarios').val())) {
                        toastr.warning('El campo Honorarios posee caracteres inválidos</br> Solo se permiten Números y </br> una coma si hay decimales');
                        flag = false;
                    }
                }
                if ($('#inputMontoAcordado').val().trim().length > 0) {
                    if (!regExp.test($('#inputMontoAcordado').val())) {
                        toastr.warning('El campo Monto Acordado posee caracteres inválidos</br> Solo se permiten Números y </br> una coma si hay decimales');
                        flag = false;
                    }
                }
                if ($('#inputArancel').val().trim().length > 0) {
                    if (!regExp.test($('#inputArancel').val())) {
                        toastr.warning('El campo Arancel posee caracteres inválidos</br> Solo se permiten Números y </br> una coma si hay decimales');
                        flag = false;
                    }
                }
                return flag;
            }

